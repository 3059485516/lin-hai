<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/yl_margin_xl">

    <com.ya02wmsj_cecoe.linhaimodule.widget.RatioImageView
        android:id="@+id/my_action"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:src="@mipmap/ya02wmsj_cecoe_icon_wsy"
        app:height_width="0.3" />


    <com.ya02wmsj_cecoe.linhaimodule.widget.RatioImageView
        android:id="@+id/do_action"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/yl_margin_xl"
        android:scaleType="centerCrop"
        android:src="@mipmap/ya02wmsj_cecoe_icon_sq"
        app:height_width="0.3" />


</LinearLayout>